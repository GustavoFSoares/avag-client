{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "paths": {
      "src/*": ["src/*"],
      "app/*": ["*"],
      "maps/*": ["src/maps/*"],
      "modules/*": ["src/modules/*"],
      "partials/*": ["src/partials/*"],
      "atoms/*": ["src/components/atoms/*"],
      "molecules/*": ["src/components/molecules/*"],
      "organisms/*": ["src/components/organisms/*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "stores/*": ["src/stores/*"],
      "libs/*": ["libs/*"]
    }
  },
  "exclude": ["dist", ".quasar", "node_modules"]
}
